{
  "name": "whalelint",
  "displayName": "WhaleLint",
  "description": "Dockerfile linter written in Go.",
  "version": "0.0.7",
  "publisher": "TamasGBarna",
  "author": {
    "name": "Tam√°s G. Barna"
  },
  "preview": true,
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.54.1"
  },
  "categories": [
    "Linters"
  ],
  "keywords": [
    "docker",
    "dockerfile",
    "container",
    "lint",
    "linter"
  ],
  "homepage": "https://github.com/cremindes/whalelint/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/cremindes/whalelint"
  },
  "activationEvents": [
    "workspaceContains:**/Dockerfile",
    "onLanguage:dockerfile"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "Dockerfile"
      },
      {
        "id": "Docker"
      }
    ]
  },
  "scripts": {
    "old-vscode:prepublish": "npm run compile",
    "old-compile": "tsc -p ./",
    "compile": "webpack --mode none",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./"
  },
  "dependencies": {
    "get-port": "^5.1.1",
    "json-rpc2": "^2.0.0",
    "vsce": "^1.99.0",
    "vscode-languageclient": "^7.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.9",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "vscode-test": "^1.6.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "logging": {
    "moduleLoad": false,
    "trace": true
  },
  "languageServerWhalelint.trace.server": {
    "scope": "window",
    "type": "string",
    "enum": [
      "off",
      "messages",
      "verbose"
    ],
    "default": "verbose"
  }
}
